		}
		,info: function(msg, options, title, bg_color, color){
            if (title == '') title = "Information!";
            if (bg_color == '') bg_color = "#BBF66F";
            if (color == '') color = "#000";
			return specialNotice(this.growler, msg, options, title, bg_color, color);
		}
		,ungrowl: function(n, o){
			removeNotice(n, o);
		}
	});

})();

